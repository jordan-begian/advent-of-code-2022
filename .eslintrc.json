{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:prettier/recommended"],
  "overrides": [],
  "parserOptions": {
    "sourceType": "module"
  },
  "plugins": ["unused-imports", "simple-import-sort"],
  "rules": {
    "no-unused-vars": "off",
    "unused-imports/no-unused-imports": "warn"
  }
}
